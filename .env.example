# =============================================================================
# RAPTOR v3.1 Configuration
# Two-worker architecture: Bot (1 instance) + Hunter (scalable)
# =============================================================================

# =============================================================================
# TELEGRAM
# =============================================================================
TELEGRAM_BOT_TOKEN=your_bot_token_here

# =============================================================================
# BAGS SOURCE (Hunter: Telegram signal ingestion)
# =============================================================================
# Enable BagsSource (Telegram channel monitoring)
# BAGS_SOURCE_ENABLED=true
# BAGS_BOT_TOKEN=your_bags_channel_bot_token
# BAGS_CHANNEL_ID=@bagsfm_signals_or_-1001234567890
# BAGS_DEDUPE_TTL_MS=60000

# =============================================================================
# SUPABASE (Postgres database + job queue)
# =============================================================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_KEY=your_service_key_here

# =============================================================================
# EVM Chains - RPC Endpoints
# =============================================================================

# BSC (BNB Smart Chain)
BSC_RPC_URL=https://bsc-dataseed.binance.org
BSC_WSS_URL=wss://bsc-ws-node.nariox.org

# Base
BASE_RPC_URL=https://mainnet.base.org
BASE_WSS_URL=wss://base-mainnet.g.alchemy.com/v2/YOUR_KEY

# Ethereum Mainnet (NEW in v2)
ETH_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY
ETH_WSS_URL=wss://eth-mainnet.g.alchemy.com/v2/YOUR_KEY

# =============================================================================
# Solana (NEW in v2)
# =============================================================================
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY
SOLANA_WSS_URL=wss://mainnet.helius-rpc.com/?api-key=YOUR_KEY
HELIUS_API_KEY=your_helius_api_key

# =============================================================================
# BAGS TRADE API (Executor: PRE_GRADUATION routing)
# =============================================================================
# Default is https://public-api-v2.bags.fm/api/v1
# BAGS_TRADE_API_URL=https://public-api-v2.bags.fm/api/v1
# Optional (if you have a key)
# BAGS_API_KEY=your_bags_api_key

# =============================================================================
# Executor Wallets (HOT WALLETS - KEEP SMALL AMOUNTS!)
# =============================================================================

# EVM Executor Private Key (same key used for BSC, Base, ETH)
EXECUTOR_PRIVATE_KEY=your_evm_private_key_here

# Solana Executor Private Key (Base58 encoded)
SOLANA_EXECUTOR_PRIVATE_KEY=your_solana_base58_private_key_here

# =============================================================================
# Fee Collection Wallets (NEW in v2)
# 1% fee is collected on all trades (buy + sell)
# =============================================================================
FEE_WALLET_EVM=0x0000000000000000000000000000000000000000
FEE_WALLET_SOLANA=YOUR_SOLANA_FEE_WALLET_ADDRESS

# =============================================================================
# Private RPC Endpoints (MEV Protection) - RECOMMENDED for production
# =============================================================================

# Flashbots for ETH Mainnet (free, no auth needed)
# Default: https://rpc.flashbots.net
# ETH_PRIVATE_RPC_URL=https://rpc.flashbots.net

# Flashbots for Base L2 (optional)
# BASE_PRIVATE_RPC_URL=https://rpc.flashbots.net/base

# bloXroute for BSC (requires account at bloxroute.com)
# BSC_PRIVATE_RPC_URL=https://bsc.rpc.blxrbdn.com
# BLOXROUTE_AUTH_HEADER=your_bloxroute_auth_header

# =============================================================================
# Optional API Keys
# =============================================================================
ALCHEMY_API_KEY=your_alchemy_key_here

# Jupiter API (Solana DEX aggregator) - optional, has public endpoint
# JUPITER_API_KEY=your_jupiter_api_key

# =============================================================================
# Launchpad Factory Addresses (Optional - defaults provided)
# =============================================================================
# four.meme on BSC
# FOUR_MEME_FACTORY=0x5c952063c7fc8610FFDB798152D69F0B9550762b

# Base launchpad (Virtuals Protocol or similar)
# BASE_LAUNCHPAD_FACTORY=0x44e3F80Eb8c9E4D2B5D6b8A88e7AD1E8B4F19b2c

# =============================================================================
# Trading Configuration (Optional - defaults shown)
# =============================================================================
# DEFAULT_TAKE_PROFIT=50
# DEFAULT_STOP_LOSS=30
# MAX_HOLD_TIME_HOURS=4
# MIN_OPPORTUNITY_SCORE=50

# =============================================================================
# Mode-specific Configuration (Optional)
# =============================================================================
# POOL_MAX_POSITION_PERCENT=30
# SOLO_MAX_POSITION_PERCENT=30
# SNIPE_SKIP_SAFETY_DEFAULT=false

# =============================================================================
# Monitoring (Optional but recommended for production)
# =============================================================================
# Sentry DSN for error tracking
# Get one at https://sentry.io
# SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx

# Log level (debug, info, warn, error)
# LOG_LEVEL=info

# =============================================================================
# RAPTOR v3.1 - ENCRYPTION (REQUIRED)
# =============================================================================
# 32-byte hex key for wallet encryption
# Generate with: openssl rand -hex 32
WALLET_ENCRYPTION_KEY=your_32_byte_hex_key_here

# =============================================================================
# RAPTOR v3.1 - HUNTER SAFETY CONTROLS
# =============================================================================
# Enable auto-execution of detected opportunities
# WARNING: Set to false until you've verified the system works correctly
AUTO_EXECUTE_ENABLED=false

# Enable CandidateConsumerLoop (launch_candidates -> trade_jobs)
# CANDIDATE_CONSUMER_ENABLED=false

# Enable on-chain Meteora DBC monitoring (optional)
# METEORA_ONCHAIN_ENABLED=false
# METEORA_PROGRAM_ID=dbcij3LWUppWqq96dh6gJWwBifmcGfLSB5D4DuSMaqN

# Enable lifecycle graduation monitor (optional)
# GRADUATION_ENABLED=false

# Enable TP/SL engine (optional)
# TPSL_ENGINE_ENABLED=false

# =============================================================================
# RAPTOR v3.1 - JOB QUEUE TUNING (Optional - defaults shown)
# =============================================================================
# Notification polling interval (ms)
# NOTIFICATION_POLL_INTERVAL_MS=1500

# Jobs to claim per poll
# JOB_CLAIM_LIMIT=5

# Job lease duration (seconds)
# JOB_LEASE_DURATION_SECONDS=30

# =============================================================================
# RAPTOR v3.1 - ADMIN CONTROLS
# =============================================================================
# Comma-separated Telegram user IDs for admin access
ADMIN_TG_IDS=123456789
